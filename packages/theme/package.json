{
  "name": "@vue-storefront/vendure-theme",
  "version": "1.0.0",
  "scripts": {
    "build": "nuxt build -m",
    "build:analyze": "nuxt build -a -m",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "precommit": "lint-staged",
    "start": "nuxt start",
    "test": "jest",
    "test:e2e": "cypress open --config-file tests/e2e/cypress.json",
    "test:e2e:generate:report": "yarn -s mochawesome-merge \"tests/e2e/report/*.json\" > \"tests/e2e/report.json\" && yarn -s marge tests/e2e/report.json -o \"tests/e2e/report\"",
    "test:e2e:hl": "cypress run --headless --config-file tests/e2e/cypress.json",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/google-fonts": "1.3.0",
    "@nuxtjs/i18n": "^7.3.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.2.1",
    "@project-serum/anchor": "github:mfrager/anchor-ts",
    "@solana/buffer-layout-utils": "^0.2.0",
    "@solana/spl-token": "^0.3.5",
    "@solana/wallet-adapter-base": "^0.9.18",
    "@solana/wallet-adapter-glow": "^0.1.13",
    "@solana/wallet-adapter-phantom": "^0.9.17",
    "@solana/wallet-adapter-solflare": "^0.6.18",
    "@solana/web3.js": "^1.66.2",
    "@storefront-ui/vue": "0.11.3",
    "@vue-storefront/middleware": "~2.4.1",
    "@vue-storefront/nuxt": "~2.4.1",
    "@vue-storefront/nuxt-theme": "~2.4.1",
    "@vue-storefront/vendure": "1.2.0",
    "awesome-phonenumber": "^2.51.2",
    "base32.js": "^0.1.0",
    "bs58": "^5.0.0",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.16.1",
    "libphonenumber-js": "^1.10.14",
    "luxon": "^3.0.4",
    "mathjs": "^11.3.2",
    "nuxt": "^2.15.7",
    "nuxt-i18n": "^6.5.0",
    "tiny-emitter": "^2.1.0",
    "uuid": "^9.0.0",
    "vee-validate": "^3.4.11",
    "vue-lazy-hydration": "~2.0.0-beta.4",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@vue/test-utils": "^1.2.2",
    "babel-jest": "^27.0.6",
    "cypress": "^8.2.0",
    "cypress-pipe": "^2.0.0",
    "cypress-tags": "^0.2.0",
    "jest": "^27.0.6",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "sass-loader": "^10.3.1",
    "ts-loader": "^8.4.0",
    "typescript": "^4.8.4",
    "vue-jest": "^4.0.0-0",
    "webpack": "^4.46.0"
  }
}
